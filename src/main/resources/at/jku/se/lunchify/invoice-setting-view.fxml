<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.jku.se.lunchify.InvoiceSettingController">
      <Label fx:id="warningText" textAlignment="CENTER" textFill="RED">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
      </Label>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="291.0" prefWidth="236.0" spacing="10.0" stylesheets="@lunchify-style.css">
            <Label prefHeight="21.0" prefWidth="192.0" text="Refundierungsbeträge:">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label prefHeight="21.0" prefWidth="192.0" text="Supermarkt" />
            <TextField fx:id="valueInvoiceSupermarket" onAction="#onChangeInvoiceValueButtonClick" prefColumnCount="0" prefHeight="21.0" prefWidth="192.0" promptText="Supermarkt" />
            <Label prefHeight="21.0" prefWidth="192.0" text="Restaurant" />
            <TextField fx:id="valueInvoiceRestaurant" onAction="#onChangeInvoiceValueButtonClick" prefHeight="21.0" prefWidth="192.0" promptText="Restaurant" />
            <Button fx:id="changeInvoiceValueButton" maxWidth="220.0" mnemonicParsing="false" onAction="#onChangeInvoiceValueButtonClick" prefHeight="31.0" prefWidth="220.0" text="Refundierungsbetrag ändern">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
</VBox>
