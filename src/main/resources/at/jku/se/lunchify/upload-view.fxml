<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="uploadView" stylesheets="@lunchify-style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <VBox alignment="CENTER" prefHeight="240.0" prefWidth="213.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="21.0" prefWidth="192.0" text="Rechnungstyp" />
            <ChoiceBox fx:id="invoiceType" prefHeight="21.0" prefWidth="192.0" />
            <Label prefHeight="21.0" prefWidth="192.0" text="Rechnungsdatum" />
            <DatePicker fx:id="invoiceDate" prefHeight="21.0" prefWidth="192.0" />
            <Label prefHeight="21.0" prefWidth="192.0" text="Rechnungsbetrag" />
            <TextField fx:id="invoiceValue" prefHeight="21.0" prefWidth="192.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <Button fx:id="invoiceUploadButton" mnemonicParsing="false" text="Upload" textAlignment="CENTER">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>

   </left>
</BorderPane>
